{
  "plugins": [
    "lodash",
    'styled-components',
    "syntax-dynamic-import",
    "transform-decorators-legacy",
  ],
  "presets": [
    ["env", { "es2015": { "modules": false } }],
    "stage-0",
    "react",
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node",
      ],
    },
  }
}
